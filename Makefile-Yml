ymlBkp=/media/yuri/Environment/Bakup-linux/home/oem/Workspace/GIT/TOTVS-DEV/


RestoreYmlAgendamento:
	cd java/SL-Agendamento$(rscMain) && cp $(ymlBkp)SL-Agendamento$(ymlBkp)application.yml
	cd java/SL-AgendamentoCore$(rscMain) && cp $(ymlBkp)SL-AgendamentoCore$(rscMain)application.yml
	cd java/SL-AgendamentoFile$(rscMain) && cp $(ymlBkp)SL-AgendamentoFile$(rscMain)application.yml
	cd java/SL-AgendamentoQuery$(rscMain) && cp $(ymlBkp)SL-AgendamentoQuery$(rscMain)application.yml

