	cd java/SL-ChecklistEvaluation$(rscMain) && cp $(ymlBkp)SL-ChecklistEvaluation$(rscMain)application.yml
	cd java/SL-ChecklistFile$(rscMain) && cp $(ymlBkp)SL-ChecklistFile$(rscMain)application.yml
	cd java/SL-ChecklistLocality$(rscMain) && cp $(ymlBkp)SL-ChecklistLocality$(rscMain)application.yml
	cd java/SL-ChecklistMobile$(rscMain) && cp $(ymlBkp)SL-ChecklistMobile$(rscMain)application.yml
	cd java/SL-ChecklistQuery$(rscMain) && cp $(ymlBkp)SL-ChecklistQuery$(rscMain)application.yml
	
